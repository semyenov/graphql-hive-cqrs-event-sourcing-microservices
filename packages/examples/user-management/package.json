{
  "name": "@examples/user-management",
  "version": "1.0.0",
  "description": "User Management Example using CQRS/Event Sourcing Framework",
  "type": "module",
  "main": "./src/server/server.ts",
  "scripts": {
    "dev": "bun --hot src/server/server.ts",
    "start": "bun src/server/server.ts",
    "build": "bun build src/server/server.ts --outdir=dist --format=esm --target=node",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "gql:generate": "gql.tada generate-output",
    "gql:check": "gql.tada check",
    "gql:persisted": "gql.tada generate-persisted --output persisted-documents.json",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "generate:all": "bun run codegen && bun run gql:generate"
  },
  "keywords": ["cqrs", "event-sourcing", "graphql", "user-management", "example"],
  "author": "CQRS Framework Team",
  "license": "MIT",
  "dependencies": {
    "@cqrs-framework/core": "workspace:*",
    "@cqrs-framework/types": "workspace:*",
    "@cqrs-framework/client": "workspace:*",
    "@cqrs-framework/graphql": "workspace:*",
    "@cqrs-framework/projections": "workspace:*",
    "@cqrs-framework/plugins": "workspace:*",
    "@cqrs-framework/advanced": "workspace:*",
    "@cqrs-framework/validation": "workspace:*",
    "@cqrs-framework/testing": "workspace:*",
    "@envelop/core": "^5.3.0",
    "@graphql-hive/envelop": "^0.36.2",
    "@graphql-tools/merge": "^9.1.1",
    "@graphql-tools/schema": "^10.0.25",
    "gql.tada": "^1.8.13",
    "graphql": "^16.11.0",
    "graphql-yoga": "^5.1.1",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}