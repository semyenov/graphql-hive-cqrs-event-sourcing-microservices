{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/app/server.ts",
    "src/app/test-framework.ts"
  ],
  "project": [
    "src/**/*.ts"
  ],
  "ignore": [
    "archive/**",
    "src/framework/generators/**",
    "src/examples/**",
    "src/types/__tests__/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "scripts/**"
  ],
  "ignoreDependencies": [
    "typescript",
    "@types/bun",
    "bun-types"
  ],
  "ignoreExportsUsedInFile": true,
  "ignoreBinaries": [
    "tsc"
  ]
}