{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/app/server.ts",
    "src/server.ts",
    "index.ts"
  ],
  "project": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "ignore": [
    "src/app/test-*.ts",
    "src/examples/**",
    "src/types/__tests__/**",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "ignoreDependencies": [
    "typescript",
    "@types/bun"
  ],
  "ignoreExportsUsedInFile": true
}